      "UserServiceVault",
      "ServiceVault"
    ],
    "ServiceTemplates": [
      "UserTemplates",
      "ServiceDeps/templates"
    ],
    "ServiceDeps": "ServiceDeps",
    "SessionStore": "SessionStore",
    "SharedROMCache": "SharedROMCache",
    "enable_shared_romcache": true,
    "service_owner": "a minisrv user",
