{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug node process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/app.js",
      "skipFiles": [
        "<node_internals/**"
      ],
      "stopOnEntry": true,
      "console": "externalTerminal"
    }
  ]
}